<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3"
    android:background="#401A47">

    <ImageView
        android:id="@+id/imagenArriba"
        android:layout_width="256px"
        android:layout_height="256px"
        android:layout_marginStart="156dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/mnombre"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/mnombre"
        android:layout_width="match_parent"
        android:layout_height="50sp"
        android:layout_marginHorizontal="70dp"
        android:layout_marginTop="156dp"
        android:background="#3E3E3E"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mprovincias"
        android:layout_width="match_parent"
        android:layout_height="50sp"
        android:layout_marginHorizontal="70dp"
        android:layout_marginTop="508dp"
        android:background="#3E3E3E"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/medad"
        android:layout_width="match_parent"
        android:layout_height="50sp"
        android:layout_marginHorizontal="70dp"
        android:layout_marginTop="364dp"
        android:background="#3E3E3E"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mtelefono"
        android:layout_width="match_parent"
        android:layout_height="50sp"
        android:layout_marginHorizontal="70dp"
        android:layout_marginTop="296dp"
        android:background="#3E3E3E"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mapellidos"
        android:layout_width="match_parent"
        android:layout_height="50sp"
        android:layout_marginHorizontal="70dp"
        android:layout_marginTop="224dp"
        android:background="#3E3E3E"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mdni"
        android:layout_width="match_parent"
        android:layout_height="50sp"
        android:layout_marginHorizontal="70dp"
        android:layout_marginTop="436dp"
        android:background="#3E3E3E"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/volver"
        android:layout_width="171dp"
        android:layout_height="57dp"
        android:layout_marginStart="120dp"
        android:layout_marginBottom="72dp"
        android:backgroundTint="@color/white"
        android:text="@string/volver"
        android:textColor="#5E3265"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>